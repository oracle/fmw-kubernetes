# This is an example property file for configOAM
#
# Common
IDSTORE_HOST: <LDAP_HOST>
IDSTORE_PORT: <LDAP_PORT>
#IDSTORE_ADMIN_PORT: 4444
#IDSTORE_KEYSTORE_FILE: OUD_INSTANCE_HOME/OUD/config/admin-keystore
#IDSTORE_KEYSTORE_PASSWORD: Password key
IDSTORE_BINDDN: <LDAP_ADMIN_USER>
IDSTORE_GROUPSEARCHBASE: <LDAP_GROUP_SEARCHBASE>
IDSTORE_SEARCHBASE: <LDAP_SEARCHBASE>
IDSTORE_USERNAMEATTRIBUTE: cn
IDSTORE_LOGINATTRIBUTE: uid
OAM_SERVER_LOGIN_ATTRIBUTE: uid
IDSTORE_USERSEARCHBASE: <LDAP_USER_SEARCHBASE>
IDSTORE_NEW_SETUP: true
IDSTORE_DIRECTORYTYPE: OUD
# OAM
IDSTORE_OAMADMINUSER: <LDAP_OAMADMIN_USER>
IDSTORE_OAMSOFTWAREUSER: <LDAP_OAMLDAP_USER>
OAM_IDSTORE_ROLE_SECURITY_ADMIN: <LDAP_OAMADMIN_GRP>
# OAM and OIM
IDSTORE_SYSTEMIDBASE: cn=<LDAP_SYSTEMIDS>,<LDAP_SEARCHBASE>
# OIM
IDSTORE_OIMADMINGROUP: <LDAP_OIGADMIN_GRP>
IDSTORE_OIMADMINUSER: <LDAP_OIGLDAP_USER>
# WebLogic
IDSTORE_WLSADMINUSER : <LDAP_WLSADMIN_USER>
IDSTORE_WLSADMINGROUP : <LDAP_WLSADMIN_GRP>
# OAM Properties
OAM_IDSTORE_NAME: OAMIDSTORE
PRIMARY_OAM_SERVERS: <OAM_DOMAIN_NAME>-oap.<OAMNS>.svc.cluster.local:<OAM_OAP_PORT>
WEBGATE_TYPE: ohsWebgate12c
ACCESS_GATE_ID: Webgate_IDM
OAM_OIM_WEBGATE_PASSWD: <LDAP_USER_PWD>
COOKIE_DOMAIN: <OAM_COOKIE_DOMAIN>
COOKIE_EXPIRY_INTERVAL: 120
OAM_WG_DENY_ON_NOT_PROTECTED: true
OAM_IDM_DOMAIN_OHS_HOST: <OAM_LOGIN_LBR_HOST>
OAM_IDM_DOMAIN_OHS_PORT: <OAM_LOGIN_LBR_PORT>
OAM_IDM_DOMAIN_OHS_PROTOCOL: <OAM_LOGIN_LBR_PROTOCOL>
OAM_SERVER_LBR_HOST: <OAM_LOGIN_LBR_HOST>
OAM_SERVER_LBR_PORT: <OAM_LOGIN_LBR_PORT>
OAM_SERVER_LBR_PROTOCOL: <OAM_LOGIN_LBR_PROTOCOL>
OAM_OAM_SERVER_TRANSFER_MODE: open
OAM_OAM_SSLENABLED: false
OAM_TRANSFER_MODE: open
OAM_SSO_ONLY_FLAG: false
OAM_IMPERSONATION_FLAG: false
OAM_IDM_DOMAIN_LOGOUT_URLS: /console/jsp/common/logout.jsp,/em/targetauth/emaslogout.jsp
OAM_OIM_INTEGRATION_REQ: <OAM_OIG_INTEG> 
OAM_OIM_OHS_URL: <OIG_LBR_PROTOCOL>://<OIG_LBR_HOST>:<OIG_LBR_PORT>/
# WebLogic Properties
WLSHOST: <OAM_DOMAIN_NAME>-adminserver.<OAMNS>.svc.cluster.local
WLSPORT: 7001
WLSADMIN: <OAM_WEBLOGIC_USER>
IDSTORE_WLSADMINUSER: <LDAP_WLSADMIN_USER>
IDSTORE_WLSADMINGROUP: <LDAP_WLSADMIN_GRP>
WLSPASSWD: <OAM_WEBLOGIC_PWD>
IDSTORE_PWD_OAMSOFTWAREUSER: <LDAP_USER_PWD>
IDSTORE_PWD_OAMADMINUSER: <LDAP_USER_PWD>
IDSTORE_PASSWD: <LDAP_ADMIN_PWD>
OAM_IDM_DOMAIN_WEBGATE_PASSWD: <LDAP_USER_PWD>
IDSTORE_SSL_ENABLED: false
WLS_IS_SSLENABLED: false
WLS_TRUSTSTORE: /u01/oracle/config/keystores/idmTrustStore.p12 
WLS_TRUSTSTORE_PASSWORD: <LDAP_USER_PWD>
WLS_SSL_HOST_VERIFICATION: true

# Logger Properties
LOG_FILE: /u01/oracle/user_projects/workdir/automation_integ.log
LOG_LEVEL: ALL
SSL_DEBUG_ENABLE: FALSE
